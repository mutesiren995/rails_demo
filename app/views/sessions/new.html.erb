
<h1>log in</h1>
<div class="row">
  <div class="span6 col-sm-offset-3 col-sm-6">
    <%= form_for(:sessions, url: sessions_path) do |f| %>
        <%= f.label :email %>
        <%= f.text_field :email %>
        <%= f.label :password %>
        <%= f.password_field :password %>
        <%= f.submit "log in " , class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
